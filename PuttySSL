cat <<EOF > /tmp/putty.sh 
source ~/.profile 
source /etc/profile 
kill -SIGKILL $(lsof -i tcp:3000 -t) 2> /dev/null 
cd /vagrant/apps/ticket_machine/ 
bundle install >/dev/null 
bundle exec bundle install >/dev/null 
echo -ne '\033]0;TicketMachine - development\007' 
echo TicketMachine Enviroment: development 
rm /tmp/putty.sh 
rails s puma -e development 
